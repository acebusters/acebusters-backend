{
  "name": "acebusters-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm --prefix=./common install",
    "lint": "eslint --fix",
    "build": "npm run build:init && npm run build:js && npm run build:install",
    "build:init": "rm -rf $PACKAGE/dist && mkdir $PACKAGE/dist",
    "build:js": "cd $PACKAGE && babel index.js --out-dir dist && babel src --out-dir dist/src",
    "build:copypack": "cd $PACKAGE && cp package.json dist/ && sed -i '' 's/..\\/common/..\\/common/g' dist/package.json",
    "build:install": "npm run build:copypack && cd $PACKAGE/dist && npm install --production && babel node_modules/ab-backend-common/*.js --out-dir .",
    "package": "npm run build && npm run package:pack",
    "package:pack": "cd $PACKAGE/dist && zip -r ../../$PACKAGE-lambda.zip *",
    "test": "./node_modules/.bin/mocha -r ./babel-register $PACKAGE/index.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parsec-labs/acebusters-backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/parsec-labs/acebusters-backend/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.9.0",
    "eslint": "^3.10.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-airbnb-base": "^11.1.2",
    "eslint-config-standard": "^7.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.6.0",
    "eslint-plugin-standard": "^2.1.1",
    "mocha": "^3.0.2"
  },
  "homepage": "https://github.com/parsec-labs/acebusters-backend#readme"
}
